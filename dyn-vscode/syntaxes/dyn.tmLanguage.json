{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Dyn",
	"patterns": [
		{ "include": "#comment" },
		{ "include": "#keyword" },
		{ "include": "#ident" }
	],
	"repository": {
		"comment": {
			"patterns": [
				{
					"name": "comment.line.double-slash.dyn",
					"match": "\/\/.*"
				},
				{
					"name": "comment.block.dyn",
					"begin": "/\\*",
					"end": "([^*]|\\*[^\/])*\\*/"
				}
			]
		},
		"keyword": {
			"name": "keyword",
			"match": "panic|assert|let|let!|if|else|iter|of|return|break|continue|import|export"
		},
		"ident": {
			"name": "variable",
			"match": "[_a-zA-Z][_0-9a-zA-Z]*"
		}
	},
	"scopeName": "source.dyn"
}